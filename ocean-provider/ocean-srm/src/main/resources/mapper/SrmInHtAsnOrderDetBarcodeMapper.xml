    <select id="findList" resultMap="BaseResultMapDto">
        select
        <include refid="BaseColumnList"/>,
        opo.purchase_order_code,bm.material_code,bm.material_name,bw.warehouse_name,
        su.user_name AS create_user_name,
        sus.user_name AS modified_user_name,
        bo.organization_name AS organization_Name
        FROM wms_in_ht_asn_order_det_barcode wihaodb
        LEFT JOIN wms_in_asn_order wiao ON wihaodb.asn_order_id = wiao.asn_order_id
        LEFT JOIN wms_in_asn_order_det wiaod ON wihaodb.asn_order_det_id = wiaod.asn_order_det_id
        LEFT JOIN om_purchase_order opo ON wiao.source_order_id = opo.purchase_order_id
        LEFT JOIN base_material bm ON wiaod.material_id = bm.material_id
        LEFT JOIN sys_user su ON wihaodb.create_user_id = su.user_id
        LEFT JOIN sys_user sus ON wihaodb.modified_user_id = sus.user_id
        LEFT JOIN base_organization bo ON wihaodb.org_id = bo.organization_id
        <where>
            <if test="asnOrderId !=null and asnOrderId !=''">
                and wihaodb.asn_order_id=#{asnOrderId}
            </if>
            <if test="asnOrderDetId !=null and asnOrderDetId!=''">
                and wihaodb.asn_order_det_id = #{asnOrderDetId}
            </if>
            <if test="materialCode !=null and materialCode !=''">
                and bm.material_code like CONCAT('%',#{materialCode},'%')
            </if>
        </where>
