  <select id="findList" resultMap="BaseResultMapDto">
    SELECT<include refid="BaseColumnList"/>,
    bm.material_name,bw.warehouse_name,bm.material_version,bm.material_desc,bmt.main_unit,bis.inventory_status_name,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    FROM wms_in_ht_in_plan_order_det wihipod
    LEFT JOIN base_material bm ON wihipod.material_id = bm.material_id
    LEFT JOIN base_material_tab bmt ON wihipod.material_id = bm.material_id
    LEFT JOIN base_inventory_status bis ON wihipod.inventory_status_id = bis.inventory_status_id
    LEFT JOIN sys_user u ON wihipod.create_user_id=u.user_id
    LEFT JOIN sys_user s ON wihipod.modified_user_id=s.user_id
    LEFT JOIN base_organization bo ON wihipod.org_id = bo.organization_id
    <where>
      <if test="orgId!=null and orgId!=''">
        and wihipod.org_id = #{orgId}
      </if>
      <if test="inPlanOrderId!=null and inPlanOrderId!=''">
        and wihipod.in_plan_order_id = #{inPlanOrderId}
      </if>
      <choose>
        <when test="inPlanOrderIds!=null and inPlanOrderIds.size > 0">
          and wihipod.in_plan_order_id in
          <foreach collection="inPlanOrderIds" item="item" separator="," open="(" close=")">
            #{item,jdbcType=BIGINT}
          </foreach>
        </when>
      </choose>
    </where>
    order by wihipod.modified_time desc
  </select>


