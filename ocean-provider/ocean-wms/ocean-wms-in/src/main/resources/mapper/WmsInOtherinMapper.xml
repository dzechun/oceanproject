<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.wms.in.mapper.WmsInOtherinMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.wms.in.WmsInOtherin">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="otherin_id" jdbcType="BIGINT" property="otherinId" />
    <result column="otherin_code" jdbcType="VARCHAR" property="otherinCode" />
    <result column="work_order_id" jdbcType="BIGINT" property="workOrderId" />
    <result column="operator_user_id" jdbcType="BIGINT" property="operatorUserId" />
    <result column="in_time" jdbcType="TIMESTAMP" property="inTime" />
    <result column="in_type" jdbcType="TINYINT" property="inType" />
    <result column="in_status" jdbcType="TINYINT" property="inStatus" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="organization_id" jdbcType="BIGINT" property="organizationId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="is_delete" jdbcType="TINYINT" property="isDelete" />
  </resultMap>

  <resultMap id="BaseResultMapDTO" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.wms.in.WmsInOtherinDto">
    <result column="work_order_code" jdbcType="VARCHAR" property="workOrderCode"/>
    <result column="operator_user_name" jdbcType="VARCHAR" property="operatorUserName"/>
    <result column="organization_code" jdbcType="VARCHAR" property="organizationCode"/>
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
  </resultMap>

  <sql id="Base_Column_List">
    oi.otherin_id,
    oi.otherin_code,
    oi.work_order_id,
    oi.operator_user_id,
    oi.in_time,
    oi.in_type,
    oi.in_status,
    oi.`status`,
    oi.remark,
    oi.organization_id,
    oi.create_user_id,
    oi.create_time,
    oi.modified_user_id,
    oi.modified_time,
    oi.is_delete
  </sql>

  <select id="findList" resultMap="BaseResultMapDTO" parameterType="map">
    SELECT <include refid="Base_Column_List"></include>,
    su.user_name AS create_user_name,
    sus.user_name AS modified_user_name,
    sy.user_name AS operator_user_name,
    bo.organization_code AS organization_code,
    swo.work_order_code AS work_order_code
    FROM
    wms_in_otherin AS oi
    LEFT JOIN ocean.sys_user su ON oi.create_user_id = su.user_id
    LEFT JOIN ocean.sys_user sus ON oi.modified_user_id = sus.user_id
    LEFT JOIN ocean.sys_user sy ON oi.operator_user_id = sy.user_id
    LEFT JOIN base_organization bo ON oi.organization_id = bo.organization_id
    LEFT JOIN smt_work_order swo ON oi.work_order_id = swo.work_order_id
    <where>
      <if test="startTime != null and startTime != '' and endTime != null and endTime != ''">
        and date_format(oi.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="otherinCode != null and otherinCode != ''">
        and oi.otherin_code like CONCAT('%', #{otherinCode}, '%')
      </if>
      <if test="workOrderCode != null and workOrderCode != ''">
        and swo.work_order_code like CONCAT('%', #{workOrderCode}, '%')
      </if>
      <if test="operatorUserName != null and operatorUserName != ''">
        and sy.user_name like CONCAT('%', #{operatorUserName}, '%')
      </if>
    </where>

  </select>
</mapper>