    su.user_name AS create_user_name,
    sus.user_name AS modified_user_name,
    bo.organization_name AS organization_Name
    FROM wms_out_delivery_req_order wodro
    LEFT JOIN base_warehouse bw ON wodro.warehouse_id = bw.warehouse_id
    LEFT JOIN sys_user su ON wodro.create_user_id = su.user_id
    LEFT JOIN sys_user sus ON wodro.modified_user_id = sus.user_id
    LEFT JOIN base_organization bo ON wodro.org_id = bo.organization_id
    <where>
      <if test="deliveryReqOrderCode!=null and deliveryReqOrderCode!=''">
        and wodro.delivery_req_order_code like CONCAT('%', #{deliveryReqOrderCode}, '%')
