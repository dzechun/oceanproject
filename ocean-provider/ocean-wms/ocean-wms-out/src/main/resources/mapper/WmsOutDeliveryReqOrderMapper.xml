      <if test="warehouseName!=null and warehouseName!=''">
        and bw.warehouse_name like CONCAT('%', #{warehouseName}, '%')
      </if>
      <if test="sourceSysOrderTypeCode != null and sourceSysOrderTypeCode!=''">
        and wodro.source_sys_order_type_code = #{sourceSysOrderTypeCode}
      </if>
      <if test="orderStatus != null">
        and wodro.order_status = #{orderStatus}
      </if>
      <if test="orgId!=null">
        and wodro.org_id = #{orgId}
      </if>
      <if test="status != null">
        and wodro.status = #{status}
      </if>
      <if test="startTime != null and startTime != '' and endTime != null and endTime != ''">
        and date_format(wodro.create_time, '%Y-%m-%d') <![CDATA[ >= ]]> #{startTime}
        and date_format(wodro.create_time, '%Y-%m-%d') <![CDATA[ <= ]]> #{endTime}
      </if>
    </where>
    order by wodro.create_time desc
