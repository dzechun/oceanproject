  <select id="findList" parameterType="map" resultMap="BaseResultMapDTO">
    SELECT <include refid="Base_Column_List"></include>,
    su.user_name AS create_user_name,
    sus.user_name AS modified_user_name,
    suy.user_name AS warehouse_user_name,
    pr.purchase_return_code,
    m.material_code,
    m.material_name,
    w.warehouse_code,
    w.warehouse_name
    FROM
    wms_out_purchase_return_det AS prt
    LEFT JOIN ocean.sys_user su  ON prt.create_user_id = su.user_id
    LEFT JOIN ocean.sys_user sus ON prt.modified_user_id = sus.user_id
    LEFT JOIN ocean.sys_user suy ON prt.warehouse_user_id = suy.user_id
    LEFT JOIN wms_out_purchase_return pr ON prt.purchase_return_id = pr.purchase_return_id
    LEFT JOIN smt_material m ON prt.material_id = m.material_id
    LEFT JOIN smt_warehouse w ON prt.warehouse_id = w.warehouse_id
  </select>
