    FROM wms_out_ht_plan_delivery_order_det wopdod
    LEFT JOIN base_material bm ON wopdod.material_id = bm.material_id
    LEFT JOIN wms_out_plan_delivery_order wopdo ON wopdod.plan_delivery_order_id = wopdo.plan_delivery_order_id
    LEFT JOIN base_warehouse bw ON wopdod.warehouse_id = bw.warehouse_id
    LEFT JOIN sys_user su ON wopdod.create_user_id = su.user_id
    LEFT JOIN sys_user sus ON wopdod.modified_user_id = sus.user_id
    LEFT JOIN base_organization bo ON wopdod.org_id = bo.organization_id
    <where>
