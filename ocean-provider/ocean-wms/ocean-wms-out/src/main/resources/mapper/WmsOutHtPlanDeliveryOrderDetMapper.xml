      </if>
      <if test="startTime != null and startTime != '' and endTime != null and endTime != ''">
        and date_format(wopdod.create_time, '%Y-%m-%d') <![CDATA[ >= ]]> #{startTime}
        and date_format(wopdod.create_time, '%Y-%m-%d') <![CDATA[ <= ]]> #{endTime}
