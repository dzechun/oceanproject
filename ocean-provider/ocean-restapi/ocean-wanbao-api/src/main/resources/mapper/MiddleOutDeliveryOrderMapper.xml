            "order_status",
            "work_order_code",
            "customer_order_code",
            "product_model_code",
            "material_code",
            "material_name",
            "picking_qty",
            "dispatch_qty",
            "option1",
            "option2",
            "option3",
            "option4",
            "option5",
            "option6" ,
            "option7",
            "option8",
            "option9" ,
            "option10",
            "option11",
            "option12" ,
            "option13",
            "option14"
        )
        VALUES
        (#{deliveryOrderId},
         #{deliveryOrderCode},
         #{planDespatchDate},
         #{actualDespatchDate},
         #{customerCode},
         #{customerName},
         #{auditStatus},
         #{orderStatus},
         #{workOrderCode},
         #{customerOrderCode},
         #{productModelCode},
         #{materialCode},
         #{materialName},
         #{pickingQty},
         #{dispatchQty},
         #{option1},
         #{option2},
         #{option3},
         #{option4},
         #{option5},
         #{option6},
         #{option7},
         #{option8},
         #{option9},
         #{option10},
         #{option11},
         #{option12},
         #{option13},
