<mapper namespace="com.fantechs.provider.wanbao.api.mapper.MiddleOutDeliveryOrderMapper">
    <resultMap id="BaseResultMap" type="com.fantechs.provider.wanbao.api.entity.MiddleOutDeliveryOrder">
