<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.mes.pm.mapper.SmtHtBarcodeRuleMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.mes.pm.history.SmtHtBarcodeRule">
    <id column="ht_barcode_rule_id" jdbcType="BIGINT" property="htBarcodeRuleId" />
    <result column="barcode_rule_id" jdbcType="BIGINT" property="barcodeRuleId" />
    <result column="barcode_rule_code" jdbcType="VARCHAR" property="barcodeRuleCode" />
    <result column="barcode_rule_name" jdbcType="VARCHAR" property="barcodeRuleName" />
    <result column="barcode_rule_desc" jdbcType="VARCHAR" property="barcodeRuleDesc" />
    <result column="barcode_rule_category_id" jdbcType="BIGINT" property="barcodeRuleCategoryId" />
    <result column="barcode_rule" jdbcType="VARCHAR" property="barcodeRule" />
    <result column="organization_id" jdbcType="BIGINT" property="organizationId" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="is_delete" jdbcType="TINYINT" property="isDelete" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />

    <result column="barcode_rule_category_name" jdbcType="VARCHAR" property="barcodeRuleCategoryName" />
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName" />
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName" />
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName" />
  </resultMap>

  <select id="findList" parameterType="com.fantechs.common.base.general.dto.mes.pm.search.SearchSmtBarcodeRule" resultMap="BaseResultMap">
    select shbr.ht_barcode_rule_id,shbr.barcode_rule_id,shbr.barcode_rule_code,shbr.barcode_rule_name,shbr.barcode_rule_desc,shbr.barcode_rule_category_id,ssi.spec_name as barcode_rule_category_name,
    shbr.barcode_rule,shbr.status,shbr.create_user_id, u.user_name as create_user_name,shbr.create_time,shbr.modified_user_id,s.user_name as modified_user_name,shbr.modified_time,
    shbr.is_delete, shbr.organization_id, shbr.remark, bo.organization_name
    from fantech_imes.smt_ht_barcode_rule shbr
    left join ocean.sys_spec_item ssi on shbr.barcode_rule_desc=ssi.spec_id
    left join ocean.sys_user u on shbr.create_user_id=u.user_id
    left join ocean.sys_user s on shbr.modified_user_id=s.user_id
    LEFT JOIN fantech_imes.base_organization bo ON bc.organization_id = bo.organization_id
    <where>
      1=1
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(shbr.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="barcodeRuleCode!=null and barcodeRuleCode!=''">
        and shbr.barcode_rule_code like CONCAT('%', #{barcodeRuleCode}, '%')
      </if>
      <if test="barcodeRuleName!=null and barcodeRuleName!=''">
        and shbr.barcode_rule_name like CONCAT('%', #{barcodeRuleName}, '%')
      </if>
      <if test="barcodeRuleDesc!=null and barcodeRuleDesc!=''">
        and shbr.barcode_rule_desc like CONCAT('%', #{barcodeRuleDesc}, '%')
      </if>
    </where>
    order by shbr.modified_time desc
  </select>
</mapper>