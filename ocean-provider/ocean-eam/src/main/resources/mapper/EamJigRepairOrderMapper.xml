    ejro.modified_time,
    ejro.option1,
    ejro.option2,
    ejro.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    ej.jig_code,ej.jig_name,ej.jig_desc,ej.jig_model,
    bbc.badness_cause_code,bbc.badness_cause_desc,
    bbd.badness_duty_code,bbd.badness_duty_desc,
    bbp.badness_phenotype_code,bbp.badness_phenotype_desc,
    su.user_name as repair_user_name,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from eam_jig_repair_order ejro
    left join eam_jig ej on ejro.jig_id = ej.jig_id
    left join base_badness_cause bbc on ejro.badness_cause_id = bbc.badness_cause_id
    left join base_badness_duty bbd on ejro.badness_duty_id = bbd.badness_duty_id
    left join base_badness_phenotype bbp on ejro.badness_phenotype_id = bbp.badness_phenotype_id
    left join sys_user su on ejro.repair_user_id=su.user_id
    left join sys_user u on ejro.create_user_id=u.user_id
    left join sys_user s on ejro.modified_user_id=s.user_id
    left join base_organization bo ON ejro.org_id = bo.organization_id
    <where>
      <if test="jigRepairOrderId!=null">
        and ejro.jig_repair_order_id = #{jigRepairOrderId}
      </if>
      <if test="jigRepairOrderCode != null and jigRepairOrderCode!=''">
        and ejro.jig_repair_order_code like CONCAT('%', #{jigRepairOrderCode}, '%')
      </if>
      <if test="jigBarcode != null and jigBarcode!=''">
        and ejro.jig_barcode like CONCAT('%', #{jigBarcode}, '%')
      </if>
      <if test="jigCode != null and jigCode!=''">
        and ej.jig_code like CONCAT('%', #{jigCode}, '%')
      </if>
      <if test="jigName != null and jigName!=''">
        and ej.jig_name like CONCAT('%', #{jigName}, '%')
      </if>
      <if test="jigModel != null and jigModel!=''">
        and ej.jig_model like CONCAT('%', #{jigModel}, '%')
      </if>
      <if test="orderStatus!=null">
        and ejro.order_status = #{orderStatus}
      </if>
      <if test="jigBarcodeId!=null">
        and ejro.jig_barcode_id = #{jigBarcodeId}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ejro.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ejro.status = #{status}
      </if>
      <if test="orgId!=null">
        and ejro.org_id=#{orgId}
      </if>
      <!-- 全表单查询 -->
      <if test="query != null">
        and #FULL_FROM_QUERY
      </if>
    </where>
    order by ejro.create_time desc
  </select>
