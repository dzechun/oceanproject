  <select id="findHtList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    ejc.jig_category_name,su.user_name,bw.warehouse_name,bwa.warehouse_area_name,bs.storage_code,
    sus.user_name as maintain_user_name,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_ht_jig ehj
    left join fantech_imes_v2.eam_jig_category ejc on ehj.jig_category_id=ejc.jig_category_id
    left join fantech_imes_v2.base_warehouse bw on ehj.warehouse_id=bw.warehouse_id
    left join fantech_imes_v2.base_warehouse_area bwa on ehj.warehouse_area_id=bwa.warehouse_area_id
    left join fantech_imes_v2.base_storage bs on ehj.storage_id=bs.storage_id
    left join ocean_v2.sys_user sus on ehj.maintain_user_id=sus.user_id
    left join ocean_v2.sys_user su on ehj.user_id=su.user_id
    left join ocean_v2.sys_user u on ehj.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ehj.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ehj.org_id = bo.organization_id
    <where>
      <if test="jigCode != null and jigCode!=''">
        and ehj.jigCode like CONCAT('%', #{jigCode}, '%')
      </if>
      <if test="jigName != null and jigName!=''">
        and ehj.jig_name like CONCAT('%', #{jigName}, '%')
      </if>
      <if test="jigModel != null and jigModel!=''">
        and ehj.jig_model like CONCAT('%', #{jigModel}, '%')
      </if>
      <if test="jigCategoryName != null and jigCategoryName!=''">
        and ehj.jig_category_name like CONCAT('%', #{jigCategoryName}, '%')
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ehj.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ehj.status = #{status}
      </if>
      <if test="orgId!=null">
        and ehj.org_id=#{orgId}
      </if>
    </where>
    order by ehj.create_time desc
  </select>
