<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamSparePartCategoryMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.EamSparePartCategory">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="spare_part_category_id" jdbcType="BIGINT" property="sparePartCategoryId" />
    <result column="spare_part_category_code" jdbcType="VARCHAR" property="sparePartCategoryCode" />
    <result column="spare_part_category_name" jdbcType="VARCHAR" property="sparePartCategoryName" />
    <result column="spare_part_category_desc" jdbcType="VARCHAR" property="sparePartCategoryDesc" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />

    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
  </resultMap>

  <sql id="Base_Column_List">
    espc.spare_part_category_id,
    espc.spare_part_category_code,
    espc.spare_part_category_name,
    espc.spare_part_category_desc,
    espc.`status`,
    espc.remark,
    espc.org_id,
    espc.create_user_id,
    espc.create_time,
    espc.modified_user_id,
    espc.modified_time,
    espc.option1,
    espc.option2,
    espc.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_spare_part_category espc
    left join ocean_v2.sys_user u on espc.create_user_id=u.user_id
    left join ocean_v2.sys_user s on espc.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON espc.org_id = bo.organization_id
    <where>
      <if test="sparePartCategoryCode != null and sparePartCategoryCode!= ''">
        and espc.spare_part_category_code like CONCAT('%',  #{sparePartCategoryCode},'%')
      </if>
      <if test="sparePartCategoryName != null and sparePartCategoryName!= ''">
        and espc.spare_part_category_name like CONCAT('%',  #{sparePartCategoryName},'%')
      </if>
      <if test="sparePartCategoryDesc != null and sparePartCategoryDesc!= ''">
        and espc.spare_part_category_desc like CONCAT('%',  #{sparePartCategoryDesc},'%')
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(espc.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null and status!= ''">
        and espc.status = #{status}
      </if>
      <if test="orgId!=null and orgId!= ''">
        and espc.org_id=#{orgId}
      </if>
    </where>
    order by espc.create_time desc
  </select>
</mapper>