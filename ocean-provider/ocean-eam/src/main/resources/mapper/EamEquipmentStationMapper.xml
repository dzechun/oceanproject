<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamEquipmentStationMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.EamEquipmentStation">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="equipment_station_id" jdbcType="BIGINT" property="equipmentStationId" />
    <result column="equipment_station_code" jdbcType="VARCHAR" property="equipmentStationCode" />
    <result column="equipment_station_name" jdbcType="VARCHAR" property="equipmentStationName" />
    <result column="map_url" jdbcType="VARCHAR" property="mapUrl" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
  </resultMap>


  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.eam.EamEquipmentStationDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
    <collection property="eamEquipmentReEsDtoList" ofType="com.fantechs.common.base.general.dto.eam.EamEquipmentReEsDto"
                column="{equipmentStationId=equipment_station_id}" select="com.fantechs.provider.eam.mapper.EamEquipmentReEsMapper.findList">
    </collection>
  </resultMap>


  <sql id="Base_Column_List">
    ees.equipment_station_id,
    ees.equipment_station_code,
    ees.equipment_station_name,
    ees.map_url,
    ees.`status`,
    ees.remark,
    ees.org_id,
    ees.create_user_id,
    ees.create_time,
    ees.modified_user_id,
    ees.modified_time,
    ees.option1,
    ees.option2,
    ees.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from eam_equipment_station ees
    left join ocean_v2.sys_user u on ees.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ees.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ees.org_id = bo.organization_id
    <where>
      <if test="status!=null">
        and ees.status = #{status}
      </if>
      <if test="orgId!=null">
        and ees.org_id=#{orgId}
      </if>
    </where>
    order by ees.create_time desc
  </select>

</mapper>