    <result column="last_time_maintain_time" jdbcType="TIMESTAMP" property="lastTimeMaintainTime" />
    <result column="current_maintain_time" jdbcType="INTEGER" property="currentMaintainTime" />
    <result column="current_maintain_usage_time" jdbcType="INTEGER" property="currentMaintainUsageTime" />
    <result column="current_maintain_usage_days" jdbcType="INTEGER" property="currentMaintainUsageDays" />
    <result column="usage_status" jdbcType="TINYINT" property="usageStatus" />
