    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
  </resultMap>

  <sql id="Base_Column_List">
    ehjb.ht_jig_barcode_id,
    ehjb.jig_barcode_id,
    ehjb.jig_id,
    ehjb.jig_barcode,
    ehjb.asset_code,
    ehjb.current_usage_time,
    ehjb.current_usage_days,
    ehjb.last_time_maintain_time,
    ehjb.current_maintain_time,
    ehjb.current_maintain_usage_time,
    ehjb.current_maintain_usage_days,
    ehjb.usage_status,
    ehjb.`status`,
    ehjb.remark,
    ehjb.org_id,
    ehjb.create_user_id,
    ehjb.create_time,
    ehjb.modified_user_id,
    ehjb.modified_time,
    ehjb.option1,
    ehjb.option2,
    ehjb.option3
  </sql>

  <select id="findHtList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_ht_jig_barcode ehjb
    left join ocean_v2.sys_user u on ehjb.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ehjb.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ehjb.org_id = bo.organization_id
    <where>
      <if test="jigId != null">
        and ehjb.jig_id = #{jigId}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ehjb.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ehjb.status = #{status}
      </if>
      <if test="orgId!=null">
        and ehjb.org_id=#{orgId}
      </if>
    </where>
    order by ehjb.create_time desc
  </select>
