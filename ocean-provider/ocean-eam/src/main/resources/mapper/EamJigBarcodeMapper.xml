  <sql id="Base_Column_List">
    ejb.jig_barcode_id,
    ejb.jig_id,
    ejb.jig_barcode,
    ejb.asset_code,
    ejb.current_usage_time,
    ejb.last_time_maintain_time,
    ejb.current_maintain_time,
