  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    ej.jig_code,ej.jig_name,ej.jig_desc,ej.jig_model,ejc.jig_category_name,ejc.jig_category_id,su.user_name,
    DATEDIFF(NOW(),ejb.create_time)+ejb.current_usage_days as current_usage_days,
    DATEDIFF(NOW(),ejb.last_time_maintain_time) as current_maintain_usage_days,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_jig_barcode ejb
    left join fantech_imes_v2.eam_jig ej on ej.jig_id=ejb.jig_id
    left join fantech_imes_v2.eam_jig_category ejc on ej.jig_category_id=ejc.jig_category_id
    left join ocean_v2.sys_user su on ej.user_id=su.user_id
    left join ocean_v2.sys_user u on ejb.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ejb.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ejb.org_id = bo.organization_id
    <where>
      <if test="jigId != null">
        and ejb.jig_id = #{jigId}
      </if>
      <if test="jigBarcode != null and jigBarcode!=''">
        and ejb.jig_barcode = #{jigBarcode}
      </if>
      <if test="assetCode != null and assetCode!=''">
        and ejb.asset_code = #{assetCode}
      </if>
      <if test="jigCode != null and jigCode!=''">
        and ej.jig_code = #{jigCode}
      </if>
      <if test="jigName != null and jigName!=''">
        and ej.jig_name = #{jigName}
      </if>
      <if test="jigDesc != null and jigDesc!=''">
        and ej.jig_desc = #{jigDesc}
      </if>
      <if test="jigModel != null and jigModel!=''">
        and ej.jig_model = #{jigModel}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ejb.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ejb.status = #{status}
      </if>
      <if test="orgId!=null">
        and ejb.org_id=#{orgId}
      </if>
      <if test="ifGroup != null and ifGroup ==1">
        group by ejb.jig_id
      </if>
    </where>
    order by ejb.create_time desc
  </select>
