    ej.option2,
    ej.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    ejc.jig_category_name,su.user_name,bw.warehouse_name,bwa.warehouse_area_name,bs.storage_code,bwo.working_area_code,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_jig ej
    left join fantech_imes_v2.eam_jig_category ejc on ej.jig_category_id=ejc.jig_category_id
    left join fantech_imes_v2.base_warehouse bw on ej.warehouse_id=bw.warehouse_id
    left join fantech_imes_v2.base_warehouse_area bwa on ej.warehouse_area_id=bwa.warehouse_area_id
    left join fantech_imes_v2.base_working_area bwo on ej.working_area_id=bwo.working_area_id
    left join fantech_imes_v2.base_storage bs on ej.storage_id=bs.storage_id
    left join ocean_v2.sys_user su on ej.user_id=su.user_id
    left join ocean_v2.sys_user u on ej.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ej.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ej.org_id = bo.organization_id
    <where>
      <if test="jigCategoryId != null">
        and ej.jig_category_id = #{jigCategoryId}
      </if>
      <if test="jigId != null">
        and ej.jig_id = #{jigId}
      </if>
      <if test="jigCode != null and jigCode!=''">
        and ej.jig_code like CONCAT('%', #{jigCode}, '%')
      </if>
      <if test="jigName != null and jigName!=''">
        and ej.jig_name like CONCAT('%', #{jigName}, '%')
      </if>
      <if test="jigModel != null and jigModel!=''">
        and ej.jig_model like CONCAT('%', #{jigModel}, '%')
      </if>
      <if test="jigCategoryName != null and jigCategoryName!=''">
        and ej.jig_category_name like CONCAT('%', #{jigCategoryName}, '%')
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ej.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ej.status = #{status}
      </if>
      <if test="orgId!=null">
        and ej.org_id=#{orgId}
      </if>
      <if test="jigBarcode!= null and jigBarcode!=''">
        and ej.jig_id in ( select jig_id from eam_jig_barcode where jig_barcode like CONCAT('%', #{jigBarcode}, '%') )
      </if>
    </where>
    order by ej.create_time desc
  </select>
