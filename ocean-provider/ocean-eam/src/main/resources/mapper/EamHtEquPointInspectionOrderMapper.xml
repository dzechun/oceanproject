  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from eam_ht_equ_point_inspection_order eepio
    left join ocean_v2.sys_user u on eepio.create_user_id=u.user_id
    left join ocean_v2.sys_user s on eepio.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON eepio.org_id = bo.organization_id
    <where>
      <if test="status!=null">
        and eepio.status = #{status}
      </if>
      <if test="orgId!=null">
        and eepio.org_id=#{orgId}
      </if>
    </where>
    order by eepio.create_time desc
  </select>
