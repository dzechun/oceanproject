<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamJigBackupMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.EamJigBackup">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="jig_backup_id" jdbcType="BIGINT" property="jigBackupId" />
    <result column="jig_backup_code" jdbcType="VARCHAR" property="jigBackupCode" />
    <result column="jig_backup_name" jdbcType="VARCHAR" property="jigBackupName" />
    <result column="jig_backup_desc" jdbcType="VARCHAR" property="jigBackupDesc" />
    <result column="warehouse_id" jdbcType="BIGINT" property="warehouseId" />
    <result column="warehouse_area_id" jdbcType="BIGINT" property="warehouseAreaId" />
    <result column="working_area_id" jdbcType="BIGINT" property="workingAreaId" />
    <result column="storage_id" jdbcType="BIGINT" property="storageId" />
    <result column="qty" jdbcType="INTEGER" property="qty" />
    <result column="jig_id" jdbcType="BIGINT" property="jigId" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
  </resultMap>

  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.eam.EamJigBackupDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
    <result column="warehouse_name" jdbcType="VARCHAR" property="warehouseName"/>
    <result column="warehouse_area_name" jdbcType="VARCHAR" property="warehouseAreaName"/>
    <result column="working_area_code" jdbcType="VARCHAR" property="workingAreaCode"/>
    <result column="storage_code" jdbcType="VARCHAR" property="storageCode"/>
  </resultMap>

  <sql id="Base_Column_List">
    ejb.jig_backup_id,
    ejb.jig_backup_code,
    ejb.jig_backup_name,
    ejb.jig_backup_desc,
    ejb.warehouse_id,
    ejb.warehouse_area_id,
    ejb.working_area_id,
    ejb.storage_id,
    ejb.qty,
    ejb.jig_id,
    ejb.`status`,
    ejb.remark,
    ejb.org_id,
    ejb.create_user_id,
    ejb.create_time,
    ejb.modified_user_id,
    ejb.modified_time,
    ejb.option1,
    ejb.option2,
    ejb.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    bw.warehouse_name,bwa.warehouse_area_name,bs.storage_code,bwo.working_area_code,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_jig_backup ejb
    left join fantech_imes_v2.base_warehouse bw on ejb.warehouse_id=bw.warehouse_id
    left join fantech_imes_v2.base_warehouse_area bwa on ejb.warehouse_area_id=bwa.warehouse_area_id
    left join fantech_imes_v2.base_working_area bwo on ejb.working_area_id=bwo.working_area_id
    left join fantech_imes_v2.base_storage bs on ejb.storage_id=bs.storage_id
    left join ocean_v2.sys_user u on ejb.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ejb.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ejb.org_id = bo.organization_id
    <where>
      <if test="jigId != null">
        and ejb.jig_id = #{jigId}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ejb.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ejb.status = #{status}
      </if>
      <if test="orgId!=null">
        and ejb.org_id=#{orgId}
      </if>
    </where>
    order by ejb.create_time desc
  </select>
</mapper>