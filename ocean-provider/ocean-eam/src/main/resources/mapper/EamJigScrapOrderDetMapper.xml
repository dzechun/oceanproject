  <sql id="Base_Column_List">
    ejsod.jig_scrap_order_det_id,
    ejsod.jig_scrap_order_id,
    ejsod.jig_barcode_id,
    ejsod.purchase_limitation_amount,
    ejsod.asset_net_value,
    ejsod.estimate_scrap_amount,
    ejsod.already_depreciation,
    ejsod.subtotal_summary,
    ejsod.scrap_description,
    ejsod.`status`,
    ejsod.remark,
    ejsod.org_id,
    ejsod.create_user_id,
