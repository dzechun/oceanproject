    ejsod.modified_user_id,
    ejsod.modified_time,
    ejsod.option1,
    ejsod.option2,
    ejsod.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    ejb.jig_barcode,ejb.asset_code,ej.jig_code,ej.jig_name,ej.jig_desc,ej.jig_model,
    ejsb.property_code_category,ejc.jig_category_name,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_jig_scrap_order_det ejsod
    left join fantech_imes_v2.eam_jig_barcode ejb on ejsod.jig_barcode_id=ejb.jig_barcode_id
    left join fantech_imes_v2.eam_jig_standing_book ejsb on ejsod.jig_barcode_id=ejsb.jig_barcode_id
    left join fantech_imes_v2.eam_jig ej on ejb.jig_id=ej.jig_id
    left join fantech_imes_v2.eam_jig_category ejc on ej.jig_category_id=ejc.jig_category_id
    left join ocean_v2.sys_user u on ejsod.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ejsod.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ejsod.org_id = bo.organization_id
    <where>
      <if test="jigScrapOrderId != null">
        and ejsod.jig_scrap_order_id = #{jigScrapOrderId}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ejsod.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ejsod.status = #{status}
      </if>
      <if test="orgId!=null">
        and ejsod.org_id=#{orgId}
      </if>
    </where>
    order by ejsod.create_time desc
  </select>
