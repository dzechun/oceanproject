<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamHtEquRepairOrderReplacementMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.history.EamHtEquRepairOrderReplacement">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="ht_equ_repair_order_replacement_id" jdbcType="BIGINT" property="htEquRepairOrderReplacementId" />
    <result column="equ_repair_order_replacement_id" jdbcType="BIGINT" property="equRepairOrderReplacementId" />
    <result column="equipment_repair_order_id" jdbcType="BIGINT" property="equipmentRepairOrderId" />
    <result column="material_id" jdbcType="BIGINT" property="materialId" />
    <result column="usage_qty" jdbcType="INTEGER" property="usageQty" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />

    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
    <result column="material_code" jdbcType="VARCHAR" property="materialCode"/>
    <result column="material_name" jdbcType="VARCHAR" property="materialName"/>
    <result column="material_desc" jdbcType="VARCHAR" property="materialDesc"/>
  </resultMap>

  <sql id="Base_Column_List">
    eheror.ht_equ_repair_order_replacement_id,
    eheror.equ_repair_order_replacement_id,
    eheror.equipment_repair_order_id,
    eheror.material_id,
    eheror.usage_qty,
    eheror.`status`,
    eheror.remark,
    eheror.org_id,
    eheror.create_user_id,
    eheror.create_time,
    eheror.modified_user_id,
    eheror.modified_time,
    eheror.option1,
    eheror.option2,
    eheror.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    bm.material_code,bm.material_name,bm.material_desc,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from eam_ht_equ_repair_order_replacement eheror
    left join base_material bm on eheror.material_id = bm.material_id
    left join sys_user u on eheror.create_user_id=u.user_id
    left join sys_user s on eheror.modified_user_id=s.user_id
    left join base_organization bo ON eheror.org_id = bo.organization_id
    <where>
      <if test="equipmentRepairOrderId != null">
        and eheror.equipment_repair_order_id = #{equipmentRepairOrderId}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(eheror.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and eheror.status = #{status}
      </if>
      <if test="orgId!=null">
        and eheror.org_id=#{orgId}
      </if>
    </where>
    order by eheror.create_time desc
  </select>
</mapper>
