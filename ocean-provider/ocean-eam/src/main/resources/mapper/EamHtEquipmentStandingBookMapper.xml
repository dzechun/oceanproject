<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamHtEquipmentStandingBookMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.history.EamHtEquipmentStandingBook">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="ht_equipment_standing_book_id" jdbcType="BIGINT" property="htEquipmentStandingBookId" />
    <result column="equipment_standing_book_id" jdbcType="BIGINT" property="equipmentStandingBookId" />
    <result column="equipment_barcode_id" jdbcType="BIGINT" property="equipmentBarcodeId" />
    <result column="property_code_category" jdbcType="TINYINT" property="propertyCodeCategory" />
    <result column="release_date" jdbcType="TIMESTAMP" property="releaseDate" />
    <result column="dept_id" jdbcType="BIGINT" property="deptId" />
    <result column="depreciable_life" jdbcType="INTEGER" property="depreciableLife" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
    <result column="attachment_path" jdbcType="LONGVARCHAR" property="attachmentPath" />
  </resultMap>

  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.eam.EamHtEquipmentStandingBookDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>

    <result column="equipment_code" jdbcType="VARCHAR" property="equipmentCode"/>
    <result column="equipment_name" jdbcType="VARCHAR" property="equipmentName"/>
    <result column="equipment_desc" jdbcType="VARCHAR" property="equipmentDesc"/>
    <result column="equipment_model" jdbcType="VARCHAR" property="equipmentModel"/>
    <result column="property_code_category" jdbcType="VARCHAR" property="propertyCodeCategory"/>
    <result column="equipment_category_desc" jdbcType="VARCHAR" property="equipmentCategoryDesc"/>

    <collection property="eamHtEquipmentStandingBookAttachments" ofType="com.fantechs.common.base.general.entity.eam.EamHtEquipmentStandingBookAttachment"
                column="{equipmentStandingBookId=equipment_standing_book_id}" select="com.fantechs.provider.eam.mapper.EamHtEquipmentStandingBookAttachmentMapper.findHtList">
    </collection>

  </resultMap>
  <sql id="BaseColumnList">
        eesb.equipment_standing_book_id,
        eesb.equipment_barcode_id,
        eesb.property_code_category,
        eesb.release_date,
        eesb.dept_id,
        eesb.depreciable_life,
        eesb.status,
        eesb.remark,
        eesb.org_id,
        eesb.create_user_id,
        eesb.create_time,
        eesb.modified_user_id,
        eesb.modified_time,
        eesb.option1,
        eesb.option2,
        eesb.option3,
        eesb.attachment_path
    </sql>

  <select id="findHtList" resultMap="BaseResultMapDto">

    SELECT<include refid="BaseColumnList"/>,
    su.user_name AS create_user_name,
    sus.user_name AS modified_user_name,bo.organization_name AS organization_name
    FROM eam_ht_equipment_standing_book eesb
    LEFT JOIN ocean_v2.sys_user su ON eesb.create_user_id = su.user_id
    LEFT JOIN ocean_v2.sys_user sus ON eesb.modified_user_id = sus.user_id
    LEFT JOIN base_organization bo ON eesb.org_id = bo.organization_id
    <where>
      <if test="orgId!=null and orgId!=''">
        and eesb.org_id = #{orgId}
      </if>
      <if test="equipmentCode!=null and equipmentCode!=''">
        and eesb.equipment_code = #{equipmentCode}
      </if>
      <if test="equipmentName!=null and equipmentName!=''">
        and eesb.equipment_name = #{equipmentName}
      </if>

    </where>
  </select>
</mapper>