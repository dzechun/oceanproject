<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamHtJigReMaterialMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.history.EamHtJigReMaterial">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="ht_jig_re_material_id" jdbcType="BIGINT" property="htJigReMaterialId" />
    <result column="jig_re_material_id" jdbcType="BIGINT" property="jigReMaterialId" />
    <result column="jig_id" jdbcType="BIGINT" property="jigId" />
    <result column="material_id" jdbcType="BIGINT" property="materialId" />
    <result column="usage_qty" jdbcType="INTEGER" property="usageQty" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />

    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
    <result column="jig_code" jdbcType="VARCHAR" property="jigCode"/>
    <result column="jig_name" jdbcType="VARCHAR" property="jigName"/>
    <result column="jig_desc" jdbcType="VARCHAR" property="jigDesc"/>
    <result column="jig_model" jdbcType="VARCHAR" property="jigModel"/>
  </resultMap>

  <sql id="Base_Column_List">
    ehjrm.ht_jig_re_material_id,
    ehjrm.jig_re_material_id,
    ehjrm.jig_id,
    ehjrm.material_id,
    ehjrm.usage_qty,
    ehjrm.`status`,
    ehjrm.remark,
    ehjrm.org_id,
    ehjrm.create_user_id,
    ehjrm.create_time,
    ehjrm.modified_user_id,
    ehjrm.modified_time,
    ehjrm.option1,
    ehjrm.option2,
    ehjrm.option3
  </sql>

  <select id="findHtList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    ej.jig_code,ej.jig_name,ej.jig_desc,ej.jig_model,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_ht_jig_re_material ehjrm
    left join fantech_imes_v2.eam_jig ej on ehjrm.jig_id=ej.jig_id
    left join ocean_v2.sys_user u on ehjrm.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ehjrm.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ehjrm.org_id = bo.organization_id
    <where>
      <if test="jigCode != null and jigCode!=''">
        and ej.jig_code like CONCAT('%', #{jigCode}, '%')
      </if>
      <if test="jigName != null and jigName!=''">
        and ej.jig_name like CONCAT('%', #{jigName}, '%')
      </if>
      <if test="jigDesc != null and jigDesc!=''">
        and ej.jig_desc like CONCAT('%', #{jigDesc}, '%')
      </if>
      <if test="jigModel != null and jigModel!=''">
        and ej.jig_model like CONCAT('%', #{jigModel}, '%')
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ehjrm.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ehjrm.status = #{status}
      </if>
      <if test="orgId!=null">
        and ehjrm.org_id=#{orgId}
      </if>
    </where>
    group by ehjrm.jig_id
    order by ehjrm.create_time desc
  </select>
</mapper>