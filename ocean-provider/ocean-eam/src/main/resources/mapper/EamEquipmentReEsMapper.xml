    <result column="equipment_mac_address" jdbcType="VARCHAR" property="equipmentMacAddress"/>
  </resultMap>


  <sql id="Base_Column_List">
    eere.equipment_re_es_id,
    eere.equipment_id,
    eere.equipment_station_id,
    eere.location_x,
    eere.location_y,
    eere.`status`,
    eere.remark,
    eere.org_id,
    eere.create_user_id,
    eere.create_time,
    eere.modified_user_id,
    eere.modified_time,
    eere.option1,
    eere.option2,
    eere.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    ee.equipment_code as equipment_code,ee.equipment_name as equipment_name,ee.equipment_model as equipment_model,
    ee.equipment_categoryId as equipment_categoryId,ee.equipment_ip as equipment_ip,ee.equipment_mac_address as equipment_mac_address,
    u.user_name as create_user_name,
    s.user_name as modified_user_name
    from eam_equipment_re_es eere
    left join eam_equipment ee on eere.equipment_id=ee.equipment_id
    left join ocean_v2.sys_user u on eere.create_user_id=u.user_id
    left join ocean_v2.sys_user s on eere.modified_user_id=s.user_id
    <where>
      <if test="equipmentId != null and equipmentId!=''">
        and eere.equipment_id = #{equipmentId}
      </if>
      <if test="equipmentStationId != null and equipmentStationId!=''">
        and eere.equipment_station_id = #{equipmentStationId}
      </if>
      <if test="status!=null">
        and eere.status = #{status}
      </if>
      <if test="orgId!=null">
        and eere.org_id=#{orgId}
      </if>
    </where>
    order by eere.create_time desc
  </select>

