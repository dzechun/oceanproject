<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamHtWiFileMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.history.EamHtWiFile">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="ht_wi_file_id" jdbcType="BIGINT" property="htWiFileId" />
    <result column="wi_file_id" jdbcType="BIGINT" property="wiFileId" />
    <result column="work_instruction_id" jdbcType="BIGINT" property="workInstructionId" />
    <result column="wi_file_org_name" jdbcType="VARCHAR" property="wiFileOrgName" />
    <result column="wi_file_name" jdbcType="VARCHAR" property="wiFileName" />
    <result column="store_path" jdbcType="VARCHAR" property="storePath" />
    <result column="access_url" jdbcType="VARCHAR" property="accessUrl" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
  </resultMap>


  <sql id="BaseColumnList">
        ehwf.ht_wi_file_id,
        ehwf.work_instruction_id,
        ehwf.wi_file_org_name,
        ehwf.wi_file_name,
        ehwf.store_path,
        ehwf.access_url,
        ehwf.status,
        ehwf.remark,
        ehwf.org_id,
        ehwf.create_user_id,
        ehwf.create_time,
        ehwf.modified_user_id,
        ehwf.modified_time,
        ehwf.option1,
        ehwf.option2,
        ehwf.option3
    </sql>

  <select id="findHtList" parameterType="map" resultMap="BaseResultMap">
    SELECT<include refid="BaseColumnList"/>
    FROM eam_ht_wi_file ehwf
    LEFT JOIN eam_ht_work_instruction ehwi ON ehwf.work_instruction_id = ehwi.work_instruction_id
    LEFT JOIN ocean_v2.sys_user su ON ehwf.create_user_id = su.user_id
    LEFT JOIN ocean_v2.sys_user sus ON ehwf.modified_user_id = sus.user_id
    LEFT JOIN base_organization bo ON ehwf.org_id = bo.organization_id
    <where>
      <if test="orgId!=null and orgId!=''">
        and ehwf.org_id = #{orgId}
      </if>
      <if test="workInstructionId!=null and workInstructionId!=''">
        and ehwf.work_instruction_id = #{workInstructionId}
      </if>
    </where>
  </select>
</mapper>