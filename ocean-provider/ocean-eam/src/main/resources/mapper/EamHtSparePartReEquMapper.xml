    eebp.spare_part_desc,
    eebp.qty,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from eam_ht_spare_part_re_equ eeb
    left join eam_spare_part eebp on eeb.spare_part_id=eebp.spare_part_id
    left join base_warehouse bw on eebp.warehouse_id=bw.warehouse_id
    left join base_warehouse_area bwa on eebp.warehouse_area_id=bwa.warehouse_area_id
    left join base_working_area bwo on eebp.working_area_id=bwo.working_area_id
    left join base_storage bs on eebp.storage_id=bs.storage_id
    left join sys_user u on eeb.create_user_id=u.user_id
    left join sys_user s on eeb.modified_user_id=s.user_id
    left join base_organization bo ON eeb.org_id = bo.organization_id
    <where>
      <if test="equipmentId != null and equipmentId!= ''">
        and eeb.equipment_id = #{equipmentId}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(eeb.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null and status!= ''">
        and eeb.status = #{status}
      </if>
      <if test="orgId!=null and orgId!= ''">
        and eeb.org_id=#{orgId}
      </if>
    </where>
    order by eeb.create_time desc
