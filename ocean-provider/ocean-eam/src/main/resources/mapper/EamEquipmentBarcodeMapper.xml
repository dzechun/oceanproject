<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamEquipmentBarcodeMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.EamEquipmentBarcode">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="equipment_barcode_id" jdbcType="BIGINT" property="equipmentBarcodeId" />
    <result column="equipment_id" jdbcType="BIGINT" property="equipmentId" />
    <result column="equipment_barcode" jdbcType="VARCHAR" property="equipmentBarcode" />
    <result column="asset_code" jdbcType="VARCHAR" property="assetCode" />
    <result column="current_usage_time" jdbcType="INTEGER" property="currentUsageTime" />
    <result column="current_usage_days" jdbcType="INTEGER" property="currentUsageDays" />
    <result column="last_time_maintain_time" jdbcType="TIMESTAMP" property="lastTimeMaintainTime" />
    <result column="current_maintain_time" jdbcType="INTEGER" property="currentMaintainTime" />
    <result column="current_maintain_usage_time" jdbcType="INTEGER" property="currentMaintainUsageTime" />
    <result column="current_maintain_usage_days" jdbcType="INTEGER" property="currentMaintainUsageDays" />
    <result column="equipment_status" jdbcType="TINYINT" property="equipmentStatus" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
  </resultMap>

  <sql id="Base_Column_List">
    eeb.equipment_barcode_id,
    eeb.equipment_id,
    eeb.equipment_barcode,
    eeb.asset_code,
    eeb.current_usage_time,
    eeb.current_usage_days,
    eeb.last_time_maintain_time,
    eeb.current_maintain_time,
    eeb.current_maintain_usage_time,
    eeb.current_maintain_usage_days,
    eeb.equipment_status,
    eeb.`status`,
    eeb.remark,
    eeb.org_id,
    eeb.create_user_id,
    eeb.create_time,
    eeb.modified_user_id,
    eeb.modified_time,
    eeb.option1,
    eeb.option2,
    eeb.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    DATEDIFF(NOW(),eeb.create_time) as current_usage_days,
    DATEDIFF(NOW(),eeb.last_time_maintain_time) as current_maintain_usage_days,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_equipment_barcode eeb
    left join ocean_v2.sys_user u on eeb.create_user_id=u.user_id
    left join ocean_v2.sys_user s on eeb.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON eeb.org_id = bo.organization_id
    <where>
      <if test="equipmentId != null">
        and eeb.equipment_id = #{equipmentId}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(eeb.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and eeb.status = #{status}
      </if>
      <if test="orgId!=null">
        and eeb.org_id=#{orgId}
      </if>
    </where>
    order by eeb.create_time desc
  </select>
</mapper>