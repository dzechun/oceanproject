  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from eam_equipment_data_group_param eedgp
    left join ocean_v2.sys_user u on eedgp.create_user_id=u.user_id
    left join ocean_v2.sys_user s on eedgp.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON eedgp.org_id = bo.organization_id
    <where>
      <if test="equipmentName != null and equipmentName!=''">
        and eedgp.equipment_data_group_name like CONCAT('%', #{equipment_data_group_name}, '%')
      </if>
      <if test="status!=null">
        and eedgp.status = #{status}
      </if>
      <if test="orgId!=null">
        and eedgp.org_id=#{orgId}
      </if>
    </where>
    order by eedgp.create_time desc
  </select>

