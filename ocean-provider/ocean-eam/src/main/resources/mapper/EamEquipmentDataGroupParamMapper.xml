  <sql id="Base_Column_List">
    eedgp.equipment_data_group_param_id,
    eedgp.equipment_data_group_id,
    eedgp.param_name,
    eedgp.field_name,
    eedgp.address_loca,
    eedgp.min_value,
    eedgp.max_value,
    eedgp.`status`,
    eedgp.remark,
    eedgp.org_id,
    eedgp.create_user_id,
