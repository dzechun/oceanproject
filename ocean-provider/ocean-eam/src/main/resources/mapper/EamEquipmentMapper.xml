<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.eam.mapper.EamEquipmentMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.eam.EamEquipment">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="equipment_id" jdbcType="BIGINT" property="equipmentId" />
    <result column="equipment_code" jdbcType="VARCHAR" property="equipmentCode" />
    <result column="equipment_name" jdbcType="VARCHAR" property="equipmentName" />
    <result column="equipment_desc" jdbcType="VARCHAR" property="equipmentDesc" />
    <result column="equipment_model" jdbcType="VARCHAR" property="equipmentModel" />
    <result column="equipment_category_id" jdbcType="BIGINT" property="equipmentCategoryId" />
    <result column="power" jdbcType="DECIMAL" property="power" />
    <result column="weight" jdbcType="DECIMAL" property="weight" />
    <result column="warehouse_id" jdbcType="BIGINT" property="warehouseId" />
    <result column="release_date" jdbcType="TIMESTAMP" property="releaseDate" />
    <result column="last_time_maintain_time" jdbcType="TIMESTAMP" property="lastTimeMaintainTime" />
    <result column="next_time_maintain_time" jdbcType="TIMESTAMP" property="nextTimeMaintainTime" />
    <result column="lifecycle" jdbcType="DECIMAL" property="lifecycle" />
    <result column="current_usage_times" jdbcType="INTEGER" property="currentUsageTimes" />
    <result column="max_usage_times" jdbcType="INTEGER" property="maxUsageTimes" />
    <result column="usage_status" jdbcType="TINYINT" property="usageStatus" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
  </resultMap>

  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.eam.EamEquipmentDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
    <result column="equipment_category_desc" jdbcType="VARCHAR" property="equipmentCategoryDesc"/>
    <result column="warehouse_name" jdbcType="VARCHAR" property="warehouseName"/>
  </resultMap>

  <sql id="Base_Column_List">
    ee.equipment_id,
    ee.equipment_code,
    ee.equipment_name,
    ee.equipment_desc,
    ee.equipment_model,
    ee.equipment_category_id,
    ee.power,
    ee.weight,
    ee.warehouse_id,
    ee.release_date,
    ee.last_time_maintain_time,
    ee.next_time_maintain_time,
    ee.lifecycle,
    ee.current_usage_times,
    ee.max_usage_times,
    ee.usage_status,
    ee.`status`,
    ee.remark,
    ee.org_id,
    ee.create_user_id,
    ee.create_time,
    ee.modified_user_id,
    ee.modified_time,
    ee.option1,
    ee.option2,
    ee.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    eec.equipment_category_desc,bw.warehouse_name,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.eam_equipment ee
    left join fantech_imes_v2.eam_equipment_category eec on ee.equipment_category_id=eec.equipment_category_id
    left join fantech_imes_v2.base_warehouse bw on ee.warehouse_id=bw.warehouse_id
    left join ocean_v2.sys_user u on ee.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ee.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ee.org_id = bo.organization_id
    <where>
      <if test="equipmentCode != null and equipmentCode!=''">
        and ee.equipment_code like CONCAT('%', #{equipmentCode}, '%')
      </if>
      <if test="equipmentName != null and equipmentName!=''">
        and ee.equipment_name like CONCAT('%', #{equipmentName}, '%')
      </if>
      <if test="equipmentDesc != null and equipmentDesc!=''">
        and ee.equipment_desc like CONCAT('%', #{equipmentDesc}, '%')
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ee.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and ee.status = #{status}
      </if>
      <if test="orgId!=null">
        and ee.org_id=#{orgId}
      </if>
    </where>
    order by ee.create_time desc
  </select>
</mapper>