<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.lizi.mapper.LiziScanBarcodeLogMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.provider.lizi.entity.LiziScanBarcodeLog">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="scan_barcode_log_id" jdbcType="BIGINT" property="scanBarcodeLogId" />
    <result column="barcode" jdbcType="VARCHAR" property="barcode" />
    <result column="material_id" jdbcType="BIGINT" property="materialId" />
    <result column="material_code" jdbcType="VARCHAR" property="materialCode" />
    <result column="material_name" jdbcType="VARCHAR" property="materialName" />
    <result column="product_model" jdbcType="VARCHAR" property="productModel" />
    <result column="batch_code" jdbcType="VARCHAR" property="batchCode" />
    <result column="qty" jdbcType="DECIMAL" property="qty" />
    <result column="production_date" jdbcType="VARCHAR" property="productionDate" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="is_delete" jdbcType="TINYINT" property="isDelete" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
  </resultMap>
  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.provider.lizi.entity.dto.LiziScanBarcodeLogDto">
    <result column="material_desc" jdbcType="VARCHAR" property="materialDesc"/>
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
  </resultMap>
  <sql id="base_column_list">
    lsbl.scan_barcode_log_id,
    lsbl.barcode,
    lsbl.material_code,
lsbl.material_name,
lsbl.product_model,
lsbl.batch_code,
lsbl.qty,
lsbl.production_date,
lsbl.status,
lsbl.remark,
lsbl.org_id,
lsbl.create_user_id,
lsbl.create_time,
lsbl.modified_user_id,
lsbl.modified_time,
lsbl.is_delete,
lsbl.option1,
lsbl.option2,
lsbl.option3
  </sql>
  <select id="findList" resultMap="BaseResultMapDto">
    select <include refid="base_column_list"/>, bm.material_code,bm.material_name,bm.material_desc,bm.option1 as idNumber,bm.option2 as skuCode,bm.option3 as sixNineCode,su.user_name as create_user_name,sus.user_name as modified_user_name
    FROM  lizi_scan_barcode_log lsbl
    LEFT JOIN base_material bm on lsbl.material_id = bm.material_id
    LEFT JOIN ocean_v2.sys_user su ON lsbl.create_user_id = su.user_id
    LEFT JOIN ocean_v2.sys_user sus ON lsbl.modified_user_id = sus.user_id
    LEFT JOIN base_organization bo ON lsbl.org_id = bo.organization_id
    <where>
      <if test="scanBarcodeLogId!=null and scanBarcodeLogId!=''">
        and lsbl.scan_barcode_log_id = #{scanBarcodeLogId}
      </if>
      <if test="materialCode!=null and materialCode!=''">
        and bm.material_code like CONCAT('%',#{materialCode},'%')
      </if>
      <if test="materialName!=null and materialName!=''">
        and bm.material_name like CONCAT('%',#{materialName},'%')
      </if>
      <if test="idNumber!=null and idNumber!=''">
        and bm.option1 like CONCAT('%',#{idNumber},'%')
      </if>
      <if test="skuCode!=null and skuCode!=''">
        and bm.option2 like CONCAT('%',#{skuCode},'%')
      </if>
      <if test="sixNineCode!=null and sixNineCode!=''">
        and bm.option3 like CONCAT('%',#{sixNineCode},'%')
      </if>
      <if test="startTime != null and startTime != '' and endTime != null and endTime != ''">
        and date_format(lsbl.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="orgId!=null and orgId!=''">
        and lsbl.org_id =#{orgId}
      </if>
    </where>
order by lsbl.create_time desc
  </select>
</mapper>