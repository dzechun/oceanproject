  </sql>
  <select id="findList" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"/>,bm.material_code,bm.material_name,bpl.pro_name,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    FROM smt_solder_paste ssp
    LEFT join base_material bm ON ssp.material_id = bm.material_id
    LEFT JOIn base_pro_line bpl ON ssp.pro_line_id = bpl.pro_line_id
    left join ocean_v2.sys_user u on ssp.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ssp.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ssp.org_id = bo.organization_id
<where>
  <if test="materialCode!=null and materialCode!=''">
    and bm.material_code like CONCAT('%',#{materialCode},'%')
  </if>
<if test="materialName!=null and materialName!=''">
  and bm.material_name like CONCAT('%',#{materialName},'%')
</if>
<if test="solderPasteStatus!=null and solderPasteStatus!=''">
  and ssp.solder_paste_status = #{solderPasteStatus}
</if>
<if test="solderPasteId!=null and solderPasteId!=''">
  and ssp.solder_paste_id = #{solderPasteId}
</if>
<if test="orgId!=null and orgId!=''">
  and ssp.org_id = #{orgId}
</if>
</where>
order by ssp.create_time
  </select>
  <select id="findInvDet" resultType="java.lang.Long">
    select material_barcode_id From wms_inner_material_barcode where barcode=#{barcode}
  </select>
  <select id="findBatchCode" resultType="java.util.Map">
    select material_id,batch_code FROM wms_inner_material_barcode where material_barcode_id=#{id}
  </select>
