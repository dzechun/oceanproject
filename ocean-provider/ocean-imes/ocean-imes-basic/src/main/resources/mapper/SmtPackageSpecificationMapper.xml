#         sps.barcode_rule_id,
#         sps.packing_unit_id,
        sps.status,
        sps.create_user_id,
        sps.create_time,
        sps.modified_user_id,
        sps.modified_time,
        sps.is_delete,
        sps.option1,
        sps.option2,
        sps.option3,
        sps.organization_id,
        sps.remark
    </sql>

    <select id="findList" resultMap="BaseResultMapDto" parameterType="map">
        select<include refid="Base_Column_List"/>,
        su.user_name AS create_user_name, sus.user_name AS modified_user_name,
#         spu.packing_unit_name, spu.packing_unit_desc,
#         sm.material_code, sm.material_desc, sm.version,
#         sp.process_code, sp.process_name, sp.process_desc,
#         sbr.barcode_rule,
        bo.organization_name
        from fantech_imes.smt_package_specification sps
#         LEFT JOIN fantech_imes.smt_material_package smp ON sps.package_specification_id = smp.package_specification_id
#         LEFT JOIN fantech_imes.smt_packing_unit spu ON smp.packing_unit_id = spu.packing_unit_id
#         LEFT JOIN fantech_imes.smt_material sm ON smp.material_id = sm.material_id
#         LEFT JOIN fantech_imes.smt_barcode_rule sbr ON smp.barcode_rule_id = sbr.barcode_rule_id
        LEFT JOIN ocean.sys_user su ON sps.create_user_id = su.user_id
        LEFT JOIN ocean.sys_user sus ON sps.modified_user_id = sus.user_id
        LEFT JOIN fantech_imes.base_organization bo ON sps.organization_id = bo.organization_id
#         LEFT JOIN fantech_imes.smt_process sp ON smp.process_id = sp.process_id
        <where>
            <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
                and date_format(sps.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
            </if>
            <if test="packageSpecificationCode != null and packageSpecificationCode != ''">
                and sps.package_specification_code like CONCAT('%', #{packageSpecificationCode}, '%')
            </if>
            <if test="packageSpecificationName != null and packageSpecificationName != ''">
                and sps.package_specification_name like CONCAT('%', #{packageSpecificationName}, '%')
            </if>
            <if test="packageSpecificationDesc != null and packageSpecificationDesc != ''">
                and sps.package_specification_desc like CONCAT('%', #{packageSpecificationDesc}, '%')
            </if>
<!--            <if test="packingUnitId != null">-->
<!--                and smp.packing_unit_id=#{packingUnitId}-->
<!--            </if>-->
<!--            <if test="packingUnitName != null and packingUnitName != ''">-->
<!--                and spu.packing_unit_name like CONCAT('%', #{packingUnitName}, '%')-->
<!--            </if>-->
<!--            <if test="packingUnitDesc != null and packingUnitDesc != ''">-->
<!--                and spu.packing_unit_desc like CONCAT('%', #{packingUnitDesc}, '%')-->
<!--            </if>-->
<!--            <if test="materialCode != null and materialCode != ''">-->
<!--                and sm.material_code like CONCAT('%', #{materialCode}, '%')-->
<!--            </if>-->
<!--            <if test="materialDesc != null and materialDesc != ''">-->
<!--                and sm.material_desc like CONCAT('%', #{materialDesc}, '%')-->
<!--            </if>-->
            <if test="status!=null">
                and sps.status=#{status}
            </if>
        </where>
        order by sps.create_time desc
    </select>
</mapper>