    <result column="organization_name" jdbcType="VARCHAR" property="organizationName" />
  </resultMap>


  <sql id="Base_Column_List">
    spos.packing_order_summary_id,
    spos.packing_order_id,
    spos.purchase_req_order_code,
    spos.contract_code,
    spos.carton_code,
    spos.profession_code,
    spos.profession_name,
    spos.material_name,
