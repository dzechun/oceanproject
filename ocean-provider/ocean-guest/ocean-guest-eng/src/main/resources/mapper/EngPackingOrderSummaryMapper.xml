    spos.net_weight,
    spos.gross_weight,
    spos.length,
    spos.height,
    spos.volume,
    spos.width,
    spos.`status`,
    spos.remark,
    spos.org_id,
    spos.create_user_id,
    spos.create_time,
    spos.modified_user_id,
    spos.modified_time,
    spos.option1,
    spos.option2,
    spos.option3,
spos.summary_status,
spos.putaway_storage_id
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"/>,
    bm.material_code,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name,
    bs.supplier_name as 'supplierName',
    spo.despatch_batch as 'despatchBatch',
    bss.storage_name as 'putawayStorageName'
    from eng_packing_order_summary spos
    LEFT JOIN eng_packing_order spo on spos.packing_order_id = spo.packing_order_id
    left join base_supplier bs on spo.supplier_id=bs.supplier_id
    left join base_material bm on spos.material_id=bm.material_id
    left join base_storage bss on spos.putaway_storage_id = bss.storage_id
    left join ocean_v2.sys_user u on spos.create_user_id=u.user_id
    left join ocean_v2.sys_user s on spos.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON spos.org_id = bo.organization_id
    <where>

      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(spos.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and spos.status = #{status}
      </if>
      <if test="packingOrderId!=null">
        and spos.packing_order_id=#{packingOrderId}
      </if>
      <if test="orgId!=null">
        and spos.org_id=#{orgId}
      </if>
      <if test="summaryStatus!=null and summaryStatus.size > 0">
        and spos.summary_status in
        <foreach collection="summaryStatus" item="item" separator="," open="(" close=")">
                #{item}
        </foreach>
      </if>
    <if test="cartonCode!=null and cartonCode!=''">
      and spos.carton_code like CONCAT('%',#{cartonCode},'%')
    </if>
      <if test="materialName!=null and materialName!=''">
        and spos.material_name like CONCAT('%',#{materialName},'%')
      </if>
    </where>
    order by spos.create_time desc
  </select>
