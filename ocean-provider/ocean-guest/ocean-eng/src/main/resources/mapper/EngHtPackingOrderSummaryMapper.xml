  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    bf.factory_name,bws.work_shop_name,bpl.pro_name,su.user_name as audit_user_name,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from eng_ht_packing_order_summary shpos
    left join ocean_v2.sys_user su on shpos.audit_user_id=su.user_id
    left join ocean_v2.sys_user u on shpos.create_user_id=u.user_id
    left join ocean_v2.sys_user s on shpos.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON shpos.org_id = bo.organization_id
    <where>

      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(shpos.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="status!=null">
        and shpos.status = #{status}
      </if>
      <if test="orgId!=null">
        and shpos.org_id=#{orgId}
      </if>
    </where>
    order by shpos.create_time desc
  </select>

