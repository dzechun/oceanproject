<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.guest.jinan.mapper.RfidAssetMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.jinan.RfidAsset">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="asset_id" jdbcType="BIGINT" property="assetId" />
    <result column="asset_code" jdbcType="VARCHAR" property="assetCode" />
    <result column="asset_name" jdbcType="VARCHAR" property="assetName" />
    <result column="asset_desc" jdbcType="VARCHAR" property="assetDesc" />
    <result column="asset_barcode" jdbcType="VARCHAR" property="assetBarcode" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />

    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
  </resultMap>

  <sql id="Base_Column_List">
    ra.asset_id,
    ra.asset_code,
    ra.asset_name,
    ra.asset_desc,
    ra.asset_barcode,
    ra.`status`,
    ra.remark,
    ra.org_id,
    ra.create_user_id,
    ra.create_time,
    ra.modified_user_id,
    ra.modified_time,
    ra.option1,
    ra.option2,
    ra.option3
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    u.user_name as create_user_name,
    s.user_name as modified_user_name,
    bo.organization_name
    from fantech_imes_v2.rfid_asset ra
    left join ocean_v2.sys_user u on ra.create_user_id=u.user_id
    left join ocean_v2.sys_user s on ra.modified_user_id=s.user_id
    left join fantech_imes_v2.base_organization bo ON ra.org_id = bo.organization_id
    <where>
      <if test="assetCode!=null and assetCode!=''">
        and ra.asset_code  like CONCAT('%', #{assetCode}, '%')
      </if>
      <if test="assetName!=null and assetName!=''">
        and ra.asset_name  like CONCAT('%', #{assetName}, '%')
      </if>
      <if test="assetDesc!=null and assetDesc!=''">
        and ra.asset_desc  like CONCAT('%', #{assetDesc}, '%')
      </if>
      <if test="assetBarcode!=null and assetBarcode!=''">
        and ra.asset_barcode  like CONCAT('%', #{assetBarcode}, '%')
      </if>
      <if test="remark!=null and remark!=''">
        and ra.remark  like CONCAT('%', #{remark}, '%')
      </if>
      <if test="status!=null">
        and ra.status = #{status}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(ra.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="orgId!=null">
        and ra.org_id=#{orgId}
      </if>
    </where>
    order by ra.create_time desc
  </select>
</mapper>