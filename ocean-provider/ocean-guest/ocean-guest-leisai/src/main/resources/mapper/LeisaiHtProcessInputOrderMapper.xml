  <select id="findList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    u.user_name as create_user_name,
    s.user_name as modified_user_name
    from leisai_ht_process_input_order lhpio
    left join ocean_v2.sys_user u on lhpio.create_user_id=u.user_id
    left join ocean_v2.sys_user s on lhpio.modified_user_id=s.user_id
    <where>
      <if test="recorder != null and recorder!=''">
        and lhpio.recorder like CONCAT('%', #{recorder}, '%')
      </if>
      <if test="processInputOrderCode != null and processInputOrderCode!=''">
        and lhpio.process_input_order_code = #{processInputOrderCode}
      </if>
      <if test="workOrderCode != null and workOrderCode!=''">
        and lhpio.work_order_code = #{workOrderCode}
      </if>
      <if test="processCode != null and processCode!=''">
        and lhpio.process_code = #{processCode}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(lhpio.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="orgId!=null">
        and lhpio.org_id=#{orgId}
      </if>

    </where>
    order by lhpio.create_time desc
  </select>

