  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.leisai.LeisaiProcessInputOrderDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName" />
  </resultMap>


  <sql id="Base_Column_List">
    lpio.process_input_order_id,
    lpio.process_input_order_code,
    lpio.finished_time,
    lpio.work_order_code,
    lpio.material_desc,
    lpio.order_qty,
    lpio.process_code,
    lpio.process_short_text,
    lpio.put_into_qty,
    lpio.badness_qty,
    lpio.test_user_count,
    lpio.hours_of_use,
    lpio.test_type,
    lpio.recorder,
    lpio.user_count_remark,
    lpio.status,
    lpio.remark,
    lpio.org_id,
    lpio.create_user_id,
    lpio.create_time,
    lpio.modified_user_id,
    lpio.modified_time,
    lpio.option1,
    lpio.option2,
    lpio.option3
  </sql>


  <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,
    u.user_name as create_user_name,
    s.user_name as modified_user_name
    from leisai_process_input_order lpio
    left join sys_user u on lpio.create_user_id=u.user_id
    left join sys_user s on lpio.modified_user_id=s.user_id
    <where>
      <if test="recorder != null and recorder!=''">
        and lpio.recorder like CONCAT('%', #{recorder}, '%')
      </if>
      <if test="processInputOrderCode != null and processInputOrderCode!=''">
        and lpio.process_input_order_code = #{processInputOrderCode}
      </if>
      <if test="workOrderCode != null and workOrderCode!=''">
        and lpio.work_order_code = #{workOrderCode}
      </if>
      <if test="processCode != null and processCode!=''">
        and lpio.process_code = #{processCode}
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(lpio.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="orgId!=null">
        and lpio.org_id=#{orgId}
      </if>

    </where>
    order by lpio.create_time desc
  </select>


