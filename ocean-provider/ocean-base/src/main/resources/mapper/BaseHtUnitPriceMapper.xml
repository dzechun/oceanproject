  <select id="findHtList" parameterType="map" resultMap="BaseResultMap">
    select<include refid="Base_Column_List"/>,
    su.user_name AS create_user_name, sus.user_name AS modified_user_name,
    sm.material_code,sm.material_name,
    spm.product_model_code,spm.product_model_name,
    bo.organization_name
    FROM fantech_imes.base_ht_unit_price bhup
    LEFT JOIN fantech_imes.smt_material sm ON bhup.material_id = sm.material_id
    LEFT JOIN fantech_imes.base_tab bt ON sm.material_id = bt.material_id
    LEFT JOIN fantech_imes.smt_product_model spm ON bt.product_model_id = spm.product_model_id
    left join ocean.sys_user su on bhup.create_user_id = su.user_id
    left join ocean.sys_user sus on bhup.modified_user_id = sus.user_id
    LEFT JOIN fantech_imes.base_organization bo ON sm.organization_id = bo.organization_id
    <where>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(bhup.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="materialCode != null and materialCode != ''">
        and sm.material_code like CONCAT('%',#{materialCode},'%')
      </if>
      <if test="materialName != null and materialName != ''">
        and sm.material_name like CONCAT('%',#{materialName},'%')
      </if>
        <if test="materialId != null">
            and sm.material_id = #{materialId}
        </if>
    </where>
  </select>
