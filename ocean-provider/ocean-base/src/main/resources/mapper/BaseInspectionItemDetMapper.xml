<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.base.mapper.BaseInspectionItemDetMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.basic.BaseInspectionItemDet">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="inspection_item_det_id" jdbcType="BIGINT" property="inspectionItemDetId" />
    <result column="inspection_item_id" jdbcType="BIGINT" property="inspectionItemId" />
    <result column="material_id" jdbcType="BIGINT" property="materialId" />
    <result column="min_package_number" jdbcType="DECIMAL" property="minPackageNumber" />
    <result column="unit" jdbcType="VARCHAR" property="unit" />
    <result column="safe_stock" jdbcType="DECIMAL" property="safeStock" />
    <result column="special_sort" jdbcType="BIGINT" property="specialSort" />
    <result column="expiration_date" jdbcType="DECIMAL" property="expirationDate" />
    <result column="rewarming_time" jdbcType="VARCHAR" property="rewarmingTime" />
    <result column="element_size" jdbcType="VARCHAR" property="elementSize" />
    <result column="humidity_class" jdbcType="BIGINT" property="humidityClass" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="organization_id" jdbcType="BIGINT" property="organizationId" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="is_delete" jdbcType="TINYINT" property="isDelete" />
  </resultMap>

  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.basic.BaseInspectionItemDetDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="material_code" jdbcType="VARCHAR" property="materialCode"/>
    <result column="material_name" jdbcType="VARCHAR" property="materialName"/>
    <result column="material_version" jdbcType="VARCHAR" property="materialVersion"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName" />
  </resultMap>

  <sql id="Base_Column_List">
    qiid.inspection_item_det_id,qiid.inspection_item_id,qiid.material_id,qiid.min_package_number,qiid.unit,
    qiid.safe_stock,qiid.special_sort,qiid.expiration_date,qiid.rewarming_time,qiid.element_size,
    qiid.humidity_class,qiid.status,qiid.create_user_id,qiid.create_time,qiid.modified_user_id,qiid.modified_time,
    qiid.is_delete, qiid.organization_id, qiid.remark
  </sql>

  <select id="findList" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>,su.user_name AS create_user_name, sus.user_name AS modified_user_name,
    sm.material_code,sm.material_name,sm.material_version,qii.inspection_item_code, bo.organization_name
    from fantech_imes_v2.base_inspection_item_det qiid
    LEFT JOIN ocean_v2.sys_user su ON qiid.create_user_id = su.user_id
    LEFT JOIN ocean_v2.sys_user sus ON qiid.modified_user_id = sus.user_id
    LEFT JOIN fantech_imes_v2.base_material sm ON qiid.material_id = sm.material_id
    LEFT JOIN fantech_imes_v2.base_inspection_item qii ON qiid.inspection_item_id = qii.inspection_item_id
    LEFT JOIN fantech_imes_v2.base_organization bo ON qiid.organization_id = bo.organization_id
    <where>
      <if test="specialSort != null and specialSort != ''">
        and qiid.special_sort like CONCAT('%', #{specialSort}, '%')
      </if>
      <if test="materialCode != null and materialCode != ''">
        and sm.material_code like CONCAT('%', #{materialCode}, '%')
      </if>
      <if test="materialName != null and materialName != ''">
        and sm.material_name like CONCAT('%', #{materialName}, '%')
      </if>
      <if test="inspectionItemCode != null and inspectionItemCode != ''">
        and qii.inspection_item_code like CONCAT('%', #{inspectionItemCode}, '%')
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(qiid.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="orgId!=null">
        and qiid.org_id=#{orgId}
      </if>
    </where>
    order by create_time desc
  </select>


</mapper>
