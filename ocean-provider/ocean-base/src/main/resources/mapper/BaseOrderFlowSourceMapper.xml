<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.base.mapper.BaseOrderFlowSourceMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.basic.BaseOrderFlowSource">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="order_flow_source_id" jdbcType="BIGINT" property="orderFlowSourceId" />
    <result column="operation_module" jdbcType="TINYINT" property="operationModule" />
    <result column="business_type" jdbcType="TINYINT" property="businessType" />
    <result column="order_flow_dimension" jdbcType="TINYINT" property="orderFlowDimension" />
    <result column="material_id" jdbcType="BIGINT" property="materialId" />
    <result column="supplier_id" jdbcType="BIGINT" property="supplierId" />
    <result column="order_type_code" jdbcType="VARCHAR" property="orderTypeCode" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="is_delete" jdbcType="TINYINT" property="isDelete" />

    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
    <result column="organization_name" jdbcType="VARCHAR" property="organizationName"/>
    <collection property="list" ofType="com.fantechs.common.base.general.entity.basic.BaseOrderFlowSourceDet"
                column="{orderFlowSourceId=order_flow_source_id}" select="com.fantechs.provider.base.mapper.BaseOrderFlowSourceDetMapper.findList">
    </collection>
  </resultMap>

  <sql id="Base_Column_List">
    bofs.order_flow_source_id,
    bofs.operation_module,
    bofs.business_type,
    bofs.order_flow_dimension,
    bofs.material_id,
    bofs.supplier_id,
    bofs.order_type_code,
    bofs.status,
    bofs.remark,
    bofs.org_id,
    bofs.create_user_id,
    bofs.create_time,
    bofs.modified_user_id,
    bofs.modified_time,
    bofs.is_delete
  </sql>

  <select id="findList" resultMap="BaseResultMap" parameterType="map">
    SELECT<include refid="Base_Column_List"/>,
    su.user_name AS create_user_name,
    sus.user_name AS modified_user_name,
    bo.organization_name
    FROM base_order_flow_source bofs
    LEFT JOIN sys_user su ON bofs.create_user_id = su.user_id
    LEFT JOIN sys_user sus ON bofs.modified_user_id = sus.user_id
    LEFT JOIN base_organization bo ON bofs.org_id = bo.organization_id
    <where>
      <if test="startTime!= null and endTime!= null">
        and date_format(bofs.create_time, '%Y-%m-%d') BETWEEN date_format(#{startTime}, '%Y-%m-%d') AND date_format(#{endTime}, '%Y-%m-%d')
      </if>
      <if test="operationModule != null and operationModule != ''">
        and bofs.operation_module = #{operationModule}
      </if>
      <if test="businessType != null and businessType != ''">
        and bofs.business_type =#{businessType}
      </if>
      <if test="orderFlowDimension!=null and orderFlowDimension != ''">
        and bofs.order_flow_dimension = #{orderFlowDimension}
      </if>
      <if test="orderTypeCode != null and orderTypeCode != ''">
        and bofs.order_type_code = #{orderTypeCode}
      </if>
      <if test="status!=null">
        and bofs.status=#{status}
      </if>
      <if test="orgId!=null">
        and bofs.org_id=#{orgId}
      </if>
    </where>
    order by bofs.create_time desc
  </select>
</mapper>