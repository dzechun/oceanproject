  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.qms.QmsQualityInspectionDetDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
