<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.qms.mapper.QmsInspectionTypeMapper">

  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.qms.QmsInspectionType">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="inspection_type_id" jdbcType="BIGINT" property="inspectionTypeId" />
    <result column="inspection_type_code" jdbcType="VARCHAR" property="inspectionTypeCode" />
    <result column="inspection_type_name" jdbcType="VARCHAR" property="inspectionTypeName" />
    <result column="inspection_type_level" jdbcType="BIGINT" property="inspectionTypeLevel" />
    <result column="inspection_tool" jdbcType="BIGINT" property="inspectionTool" />
    <result column="inspection_standard" jdbcType="VARCHAR" property="inspectionStandard" />
    <result column="inspection_nape" jdbcType="BIGINT" property="inspectionNape" />
    <result column="batch" jdbcType="DECIMAL" property="batch" />
    <result column="aql" jdbcType="DECIMAL" property="aql" />
    <result column="sampling_amount" jdbcType="DECIMAL" property="samplingAmount" />
    <result column="ac" jdbcType="DECIMAL" property="ac" />
    <result column="re" jdbcType="DECIMAL" property="re" />
    <result column="test_method" jdbcType="BIGINT" property="testMethod" />
    <result column="test_times" jdbcType="DECIMAL" property="testTimes" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="is_delete" jdbcType="TINYINT" property="isDelete" />
  </resultMap>

  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.general.dto.qms.QmsInspectionTypeDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
  </resultMap>

  <sql id="Base_Column_List">
    qit.inspection_type_id,qit.inspection_type_code,qit.inspection_type_name,qit.inspection_type_level,
    qit.inspection_tool,qit.inspection_standard,qit.inspection_nape,qit.batch,qit.aql,qit.sampling_amount,
    qit.ac,qit.re,qit.test_method,qit.test_times,qit.remark,qit.status,qit.create_user_id,qit.create_time,
    qit.modified_user_id,qit.modified_time,qit.is_delete
  </sql>

  <select id="findList" resultMap="BaseResultMapDto">
    select <include refid="Base_Column_List"></include>, su.user_name AS create_user_name, sus.user_name AS modified_user_name
    from fantech_imes.qms_inspection_type qit
    LEFT JOIN ocean.sys_user su ON qit.create_user_id = su.user_id
    LEFT JOIN ocean.sys_user sus ON qit.modified_user_id = sus.user_id
    <where>
      <if test="inspectionTypeCode != null and inspectionTypeCode != ''">
        and qit.inspection_type_code like CONCAT('%', #{inspectionTypeCode}, '%')
      </if>
      <if test="inspectionTypeName != null and inspectionTypeName != ''">
        and qit.inspection_type_name like CONCAT('%', #{inspectionTypeName}, '%')
      </if>

      <if test="inspectionTypeLevel != null and inspectionTypeLevel != ''">
        and qit.inspection_type_level like CONCAT('%', #{inspectionTypeLevel}, '%')
      </if>
      <if test="inspectionNape != null and inspectionNape != ''">
        and qit.inspection_nape like CONCAT('%', #{inspectionNape}, '%')
      </if>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(qit.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
    </where>
    order by create_time desc
  </select>

  <select id="getMax" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>
    from fantech_imes.qms_inspection_type qit ORDER BY qit.inspection_type_id desc limit 1
  </select>


</mapper>
