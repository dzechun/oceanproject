  <select id="findList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    bisd.inspection_item,bisd.inspection_tag,bisd.specification_upper_limit,bisd.specification_floor,bisd.unit_name,
