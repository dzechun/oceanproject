<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.electronic.mapper.SmtElectronicTagControllerMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.entity.SmtElectronicTagController">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="electronic_tag_controller_id" jdbcType="BIGINT" property="electronicTagControllerId" />
    <result column="electronic_tag_controller_code" jdbcType="VARCHAR" property="electronicTagControllerCode" />
    <result column="electronic_tag_controller_name" jdbcType="VARCHAR" property="electronicTagControllerName" />
    <result column="electronic_tag_controller_desc" jdbcType="VARCHAR" property="electronicTagControllerDesc" />
    <result column="electronic_tag_controller_ip" jdbcType="VARCHAR" property="electronicTagControllerIp" />
    <result column="electronic_tag_controller_port" jdbcType="VARCHAR" property="electronicTagControllerPort" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="is_delete" jdbcType="TINYINT" property="isDelete" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
  </resultMap>

  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.dto.SmtElectronicTagControllerDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName" />
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName" />
  </resultMap>

  <sql id="Base_Column_List">
    setc.electronic_tag_controller_id, setc.electronic_tag_controller_code, setc.electronic_tag_controller_name,
    setc.electronic_tag_controller_desc, setc.electronic_tag_controller_ip, setc.electronic_tag_controller_port, setc.status,
    setc.create_user_id, setc.create_time, setc.modified_user_id, setc.modified_time, setc.is_delete, setc.option1, setc.option2, setc.option3
  </sql>

  <select id="findList" resultMap="BaseResultMapDto" parameterType="map">
    SELECT <include refid="Base_Column_List"/>, su.user_name AS create_user_name, sus.user_name AS modified_user_name
    FROM smt_electronic_tag_controller setc
    LEFT JOIN ocean.sys_user su  ON setc.create_user_id = su.user_id
    LEFT JOIN ocean.sys_user sus ON setc.modified_user_id = sus.user_id
    <where>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(setc.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="electronicTagControllerCode != null">
        and setc.electronic_tag_controller_code like CONCAT('%', #{electronicTagControllerCode}, '%')
      </if>
      <if test="electronicTagControllerName != null">
        and setc.electronic_tag_controller_name like CONCAT('%', #{electronicTagControllerName}, '%')
      </if>
      <if test="electronicTagControllerDesc != null">
        and setc.electronic_tag_controller_desc like CONCAT('%', #{electronicTagControllerDesc}, '%')
      </if>
      <if test="electronicTagControllerIp != null">
        and setc.electronic_tag_controller_ip like CONCAT('%', #{electronicTagControllerIp}, '%')
      </if>
      <if test="status!=null and status!=''">
        and setc.status=#{status}
      </if>
    </where>
    order by  setc.create_time desc
  </select>
</mapper>