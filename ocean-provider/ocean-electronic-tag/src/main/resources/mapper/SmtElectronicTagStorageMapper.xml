    <sql id="Base_column_List">
    sets.electronic_tag_storage_id,
	sets.storage_id,
	sets.electronic_tag_controller_id,
	sets.electronic_tag_id,
	sets.`status`,
	sets.create_user_id,
	sets.create_time,
	sets.modified_user_id,
	sets.modified_time,
	sets.is_delete,
	sets.option1,
	sets.option2,
	sets.option3
    </sql>

    <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
        SELECT<include refid="Base_column_List"/>,
        su.user_name AS create_user_name, sus.user_name AS modified_user_name,
        ss.storage_name AS storage_name, ss.storage_desc AS storage_desc, ss.storage_code AS storage_code,
        setc.electronic_tag_controller_name AS electronic_tag_controller_name,
        setc.electronic_tag_controller_code AS electronic_tag_controller_code,
        setc.electronic_tag_controller_ip AS electronic_tag_controller_ip,
        setc.electronic_tag_controller_port AS electronic_tag_controller_port,
        sw.warehouse_name AS warehouse_name, sw.warehouse_code AS warehouse_code,
        swa.warehouse_area_name AS warehouse_area_name, swa.warehouse_area_code AS warehouse_area_code
        FROM smt_electronic_tag_storage sets
        LEFT JOIN ocean.sys_user su ON sets.create_user_id = su.user_id
        LEFT JOIN ocean.sys_user sus ON sets.modified_user_id = sus.user_id
        LEFT JOIN smt_storage ss ON sets.storage_id = ss.storage_id
        LEFT JOIN smt_electronic_tag_controller setc ON sets.electronic_tag_controller_id =
        setc.electronic_tag_controller_id
        LEFT JOIN smt_warehouse sw ON ss.warehouse_id = sw.warehouse_id
        LEFT JOIN smt_warehouse_area swa ON ss.warehouse_area_id = swa.warehouse_area_id
        <where>
            <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
                and date_format(sets.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
            </if>
            <if test="storageName != null">
                and ss.storage_name like CONCAT('%', #{storageName}, '%')
            </if>
            <if test="warehouseName != null">
                and sw.warehouse_name like CONCAT('%', #{warehouseName}, '%')
            </if>
            <if test="warehouseAreaName != null">
                and swa.warehouse_area_name like CONCAT('%', #{warehouseAreaName}, '%')
            </if>
            <if test="electronicTagControllerName != null">
                and setc.electronic_tag_controller_name like CONCAT('%', #{electronicTagControllerName}, '%')
            </if>
            <if test="electronicTagControllerId != null">
                and setc.electronic_tag_controller_id = #{electronicTagControllerId}
            </if>
            <if test="electronicTagControllerIp != null">
                and setc.electronic_tag_controller_ip like CONCAT('%', #{electronicTagControllerIp}, '%')
            </if>
            <if test="electronicTagId != null">
                and sets.electronic_tag_id like CONCAT('%', #{electronicTagId}, '%')
            </if>
            <if test="status!=null and status!=''">
                and sets.status=#{status}
            </if>
        </where>
        order by sets.create_time desc
    </select>

    <select id="findStorageByCode" parameterType="java.lang.String"
            resultMap="SmtStorageMap">
        SELECT *
        FROM smt_storage ss
        <where>
            ss.storage_code =#{storageCode}
        </where>
    </select>

    <select id="findByElectronicTagControllerId" parameterType="java.lang.Long"
            resultMap="SmtStorageMap">
        SELECT ss.*
        FROM smt_electronic_tag_storage sets
	    LEFT JOIN smt_storage ss ON sets.storage_id = ss.storage_id
        WHERE sets.electronic_tag_controller_id = #{_parameter,jdbcType=BIGINT}
    </select>

    <select id="findElectronicTagControllerByCode" parameterType="java.lang.String"
            resultMap="SmtElectronicTagControllerMap">
        SELECT *
        FROM smt_electronic_tag_controller setc
        <where>
            setc.electronic_tag_controller_code =#{electronicTagControllerCode}
        </where>
    </select>
