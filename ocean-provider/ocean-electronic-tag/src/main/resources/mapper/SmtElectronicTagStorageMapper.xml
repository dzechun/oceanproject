<mapper namespace="com.fantechs.provider.electronic.mapper.SmtElectronicTagStorageMapper">
    <resultMap id="BaseResultMap" type="com.fantechs.common.base.electronic.entity.history.SmtHtElectronicTagStorage">
        <!--
          WARNING - @mbg.generated
        -->
        <id column="electronic_tag_storage_id" jdbcType="BIGINT" property="electronicTagStorageId"/>
        <result column="storage_id" jdbcType="BIGINT" property="storageId"/>
        <result column="storage_code" jdbcType="VARCHAR" property="storageCode"/>
        <result column="equipment_id" jdbcType="BIGINT" property="equipmentId"/>
        <result column="equipment_area_id" jdbcType="VARCHAR" property="equipmentAreaId"/>
        <result column="warehouse_id" jdbcType="VARCHAR" property="warehouseId"/>
        <result column="warehouse_code" jdbcType="VARCHAR" property="warehouseCode"/>
        <result column="warehouse_area_id" jdbcType="VARCHAR" property="warehouseAreaId"/>
        <result column="warehouse_area_code" jdbcType="VARCHAR" property="warehouseAreaCode"/>
        <result column="material_id" jdbcType="VARCHAR" property="materialId" />
        <result column="material_code" jdbcType="VARCHAR" property="materialCode" />

        <result column="status" jdbcType="TINYINT" property="status"/>
        <result column="create_user_id" jdbcType="BIGINT" property="createUserId"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId"/>
        <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime"/>
        <result column="is_delete" jdbcType="TINYINT" property="isDelete"/>
        <result column="option1" jdbcType="VARCHAR" property="option1"/>
        <result column="option2" jdbcType="VARCHAR" property="option2"/>
        <result column="option3" jdbcType="VARCHAR" property="option3"/>
    </resultMap>

    <resultMap id="BaseResultMapDto" type="com.fantechs.common.base.electronic.dto.SmtElectronicTagStorageDto"
               extends="BaseResultMap">
        <result column="storage_desc" jdbcType="VARCHAR" property="storageDesc"/>
        <result column="storage_name" jdbcType="VARCHAR" property="storageName"/>
        <result column="warehouse_name" jdbcType="VARCHAR" property="warehouseName"/>
        <result column="warehouse_area_name" jdbcType="VARCHAR" property="warehouseAreaName"/>
        <result column="material_name" jdbcType="VARCHAR" property="materialName" />
        <result column="material_desc" jdbcType="VARCHAR" property="materialDesc" />
        <result column="create_user_name" jdbcType="VARCHAR" property="createUserName"/>
        <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName"/>
        <result column="equipment_name" jdbcType="VARCHAR" property="equipmentName"/>
        <result column="equipment_code" jdbcType="VARCHAR" property="equipmentCode"/>
        <result column="equipment_ip" jdbcType="VARCHAR" property="equipmentIp"/>
        <result column="equipment_port" jdbcType="VARCHAR" property="equipmentPort"/>
        <result column="queue_name" jdbcType="VARCHAR" property="queueName" />
    </resultMap>

    <sql id="Base_column_List">
    sets.electronic_tag_storage_id,
	sets.storage_id,
	sets.storage_code,
	sets.warehouse_id, sets.warehouse_code,
	sets.warehouse_area_id, sets.warehouse_area_code,
	sets.material_id, sets.material_code,
	sets.equipment_id,
	sets.equipment_area_id,
	sets.electronic_tag_id,
	sets.`status`,
	sets.create_user_id,
	sets.create_time,
	sets.modified_user_id,
	sets.modified_time,
	sets.is_delete,
	sets.option1,
	sets.option2,
	sets.option3
    </sql>

    <select id="findList" parameterType="map" resultMap="BaseResultMapDto">
        SELECT<include refid="Base_column_List"/>,
        sus.user_name,
        ss.storage_desc,
        ss.storage_name,
        swa.warehouse_area_name,
        sw.warehouse_name,
        sm.material_desc,
        sm.material_name,
        se.equipment_name,
        se.equipment_code,
        se.equipment_ip,
        se.equipment_port,
        se.equipment_type,
        scm.queue_name
        FROM smt_electronic_tag_storage sets
        LEFT JOIN ocean.sys_user su ON sets.create_user_id = su.user_id
        LEFT JOIN ocean.sys_user sus ON sets.modified_user_id = sus.user_id
        LEFT JOIN smt_equipment se ON sets.equipment_id = se.equipment_id
        LEFT JOIN smt_client_manage scm ON scm.client_id = se.client_id
        LEFT JOIN smt_storage ss ON sets.storage_id = ss.storage_id
        LEFT JOIN smt_material sm ON sets.material_id = sm.material_id
        LEFT JOIN smt_warehouse sw ON sets.warehouse_id = sw.warehouse_id
        LEFT JOIN smt_warehouse_area swa ON sets.warehouse_area_id = swa.warehouse_area_id
        <where>
            <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
                and date_format(shets.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
            </if>
            <if test="storageId != null">
                and sets.storage_id =#{storageId}
            </if>
            <if test="storageName != null">
                and ss.storage_name like CONCAT('%', #{storageName}, '%')
            </if>
            <if test="warehouseName != null">
                and sw.warehouse_name like CONCAT('%', #{warehouseName}, '%')
            </if>
            <if test="warehouseAreaName != null">
                and swa.warehouse_area_name like CONCAT('%', #{warehouseAreaName}, '%')
            </if>
            <if test="equipmentName != null">
                and se.equipment_name like CONCAT('%', #{equipmentName}, '%')
            </if>
            <if test="equipmentId != null">
                and se.equipment_Id = #{equipmentId}
            </if>
            <if test="equipmentIp != null">
                and se.equipment_ip like CONCAT('%', #{equipmentIp}, '%')
            </if>
            <if test="electronicTagId != null">
                and sets.electronic_tag_id like CONCAT('%', #{electronicTagId}, '%')
            </if>
            <if test="status!=null and status!=''">
                and sets.status=#{status}
            </if>
        </where>
        order by sets.create_time desc
    </select>
</mapper>