<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.electronic.mapper.SmtClientManageMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.electronic.entity.SmtClientManage">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="client_id" jdbcType="BIGINT" property="clientId" />
    <result column="client_name" jdbcType="VARCHAR" property="clientName" />
    <result column="client_remark" jdbcType="VARCHAR" property="clientRemark" />
    <result column="client_type" jdbcType="VARCHAR" property="clientType" />
    <result column="online_status" jdbcType="CHAR" property="onlineStatus" />
    <result column="login_ip" jdbcType="VARCHAR" property="loginIp" />
    <result column="secret_key" jdbcType="VARCHAR" property="secretKey" />
    <result column="queue_name" jdbcType="VARCHAR" property="queueName" />
    <result column="queue_address" jdbcType="VARCHAR" property="queueAddress" />
    <result column="login_time" jdbcType="TIMESTAMP" property="loginTime" />
    <result column="offline_time" jdbcType="TIMESTAMP" property="offlineTime" />
    <result column="monitoring_time" jdbcType="TIMESTAMP" property="monitoringTime" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
    <result column="is_delete" jdbcType="TINYINT" property="isDelete" />
    <result column="option1" jdbcType="VARCHAR" property="option1" />
    <result column="option2" jdbcType="VARCHAR" property="option2" />
    <result column="option3" jdbcType="VARCHAR" property="option3" />
  </resultMap>

  <resultMap id="BaseResultMapDto" extends="BaseResultMap" type="com.fantechs.common.base.electronic.dto.SmtClientManageDto">
    <result column="create_user_name" jdbcType="VARCHAR" property="createUserName" />
    <result column="modified_user_name" jdbcType="VARCHAR" property="modifiedUserName" />
  </resultMap>

  <sql id="Base_Column_List">
    scm.client_id, scm.client_name, scm.client_remark, scm.client_type, scm.online_status, scm.login_ip,
    scm.secret_key, scm.queue_name, scm.queue_address, scm.login_time, scm.offline_time, scm.monitoring_time, scm.status, scm.create_user_id,
    scm.create_time, scm.modified_user_id, scm.modified_time, scm.is_delete, scm.option1, scm.option2, scm.option3
  </sql>

  <select id="findList"  resultMap="BaseResultMapDto" parameterType="map">
    select <include refid="Base_Column_List"/>,
    su.user_name AS create_user_name, sus.user_name AS modified_user_name
    from fantech_imes.smt_client_manage scm
    LEFT JOIN ocean.sys_user su  ON scm.create_user_id = su.user_id
    LEFT JOIN ocean.sys_user sus ON scm.modified_user_id = sus.user_id
    <where>
      <if test="startTime!= null and startTime!= '' and endTime!= null and endTime!= ''">
        and date_format(scm.create_time, '%Y-%m-%d') BETWEEN #{startTime} AND #{endTime}
      </if>
      <if test="clientName != null">
        and scm.client_name like CONCAT('%', #{clientName}, '%')
      </if>
      <if test="clientRemark != null">
        and scm.client_remark like CONCAT('%', #{clientRemark}, '%')
      </if>
      <if test="clientType!=null and clientType!=''">
        and scm.client_type = #{clientType}
      </if>
      <if test="secretKey!=null and secretKey!=''">
        and scm.secret_key=#{secretKey}
      </if>
      <if test="status!=null and status!=''">
        and scm.status=#{status}
      </if>
    </where>
    order by  scm.create_time desc
  </select>
</mapper>