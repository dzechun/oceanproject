<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fantechs.provider.tem.mapper.TemHtVehicleMapper">
  <resultMap id="BaseResultMap" type="com.fantechs.common.base.general.entity.tem.history.TemHtVehicle">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="ht_vehicle_id" jdbcType="BIGINT" property="htVehicleId" />
    <result column="vehicle_id" jdbcType="BIGINT" property="vehicleId" />
    <result column="vehicle_code" jdbcType="VARCHAR" property="vehicleCode" />
    <result column="vehicle_name" jdbcType="VARCHAR" property="vehicleName" />
    <result column="standard_capacity" jdbcType="DECIMAL" property="standardCapacity" />
    <result column="agv_task_template" jdbcType="VARCHAR" property="agvTaskTemplate" />
    <result column="agv_task_template_second" jdbcType="VARCHAR" property="agvTaskTemplate" />
    <result column="storage_task_point_id" jdbcType="BIGINT" property="storageTaskPointId" />
    <result column="vehicle_status" jdbcType="TINYINT" property="vehicleStatus" />
    <result column="status" jdbcType="TINYINT" property="status" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="org_id" jdbcType="BIGINT" property="orgId" />
    <result column="create_user_id" jdbcType="BIGINT" property="createUserId" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="modified_user_id" jdbcType="BIGINT" property="modifiedUserId" />
    <result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
  </resultMap>

  <sql id="Base_Column_List">
    thv.ht_vehicle_id, thv.vehicle_id, thv.vehicle_code, thv.vehicle_name, thv.standard_capacity, thv.agv_task_template, thv.agv_task_template_second,
    thv.storage_task_point_id, thv.vehicle_status, thv.status, thv.remark, thv.org_id, thv.create_user_id, thv.create_time, thv.modified_user_id, thv.modified_time
  </sql>

  <select id="findList" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"></include>,
    suc.user_code as createUserCode,
    sum.user_code as modifiedUserCode
    from tem_ht_vehicle thv
    LEFT JOIN sys_user suc ON thv.create_user_id = suc.user_id
    LEFT JOIN sys_user sum ON thv.modified_user_id = sum.user_id
    LEFT JOIN base_storage_task_point bstp ON thv.storage_task_point_id = bstp.storage_task_point_id
    LEFT JOIN base_storage bs ON bstp.storage_id = bs.storage_id
    <where>
      <if test="vehicleCode != null and vehicleCode != ''">
        and thv.vehicle_code like CONCAT('%', #{vehicleCode}, '%')
      </if>
      <if test="vehicleName != null and vehicleName != ''">
        and thv.vehicle_name like CONCAT('%', #{vehicleName}, '%')
      </if>
      <if test="remark != null and remark != ''">
        and thv.remark like CONCAT('%', #{remark}, '%')
      </if>
      <if test="vehicleStatus != null">
        and thv.vehicle_status = #{vehicleStatus}
      </if>
      <if test="startTime != null and startTime != '' and endTime != null and endTime != ''">
        and thv.create_time between #{startTime} and #{endTime}
      </if>
    </where>
  </select>
</mapper>
